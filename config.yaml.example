# Common OCI authentication parameters (using YAML anchor)
oci_auth: &oci_auth
  drop_params: true  # Automatically drop unsupported parameters (like max_retries)
  oci_user: <your_oci_user_ocid>
  oci_fingerprint: <your_oci_fingerprint>
  oci_tenancy: <your_oci_tenancy_ocid>
  oci_region: us-chicago-1  # or your preferred region
  oci_key_file: /path/to/your/oci_api_key.pem  # Use absolute path (tilde ~ not expanded)
  oci_compartment_id: <your_oci_compartment_ocid>
  oci_serving_mode: ON_DEMAND  # Optional: ON_DEMAND or DEDICATED

model_list:
  # xAI Grok Models
  - model_name: oci/xai.grok-4
    litellm_params:
      <<: *oci_auth
      model: oci/xai.grok-4
  
